#include <stdio.h>>

int main() {
  int a; // 변수를 정합니다.

  printf("어떤 구구단을 보여드릴까요?"); // 문장을 넣어 안내하여 변수를 정할 수 있습니다.
  scanf("%d", &a); // scanf로 변수 a를 입력시킵니다.

  printf("========%d단========\n",a); // 어떤 구구단인지 표시합니다.
  for(int i=1; i<=9 ;i++) // 구구단에 시작인 1단부터 9단까지 정하고 'i'를 1씩 올려서 1단부터 9단까지 표시합니다. 
    {
      printf("%d*%d = %d\n",a,i,a*i); // '%d'에 변수 a를 넣어줍니다.
      // 두 번째'%d'에 'i'를 넣어 1단부터 9단까지 정해줍니다.
      // 세 번째'%d'에 공식을 넣어줍니다.
    }
  
}
